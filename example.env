# Notion OAuth配置
# 请创建一个.env文件并填入实际值
NOTION_CLIENT_ID=your_notion_client_id
NOTION_CLIENT_SECRET=your_notion_client_secret
NOTION_REDIRECT_URI=https://your-vercel-app.vercel.app/api/notion/callback

# 前端可访问的环境变量 (必须以NEXT_PUBLIC_开头)
NEXT_PUBLIC_NOTION_CLIENT_ID=your_notion_client_id
NEXT_PUBLIC_NOTION_REDIRECT_URI=https://your-vercel-app.vercel.app/api/notion/callback 